{
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "workbench.colorTheme": "An Old Hope Classic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/dist": true,
    "**/tmp": true
  },
  "window.zoomLevel": 0
}